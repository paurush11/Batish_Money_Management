{
  "name": "batish_money_management",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "docker:dev": "docker start batish-app && yarn run prettier",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs",
    "docker:restart": "docker-compose down && docker-compose up -d --build",
    "build": "next build",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "prettier --write 'src/**/*.{js,ts,tsx,jsx}' && next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "prettier --write 'src/**/*.{js,ts,tsx,jsx}' && next start",
    "migrate": "prisma migrate dev --name init",
    "gen": "graphql-codegen --config gen.ts",
    "prettier": "prettier --write 'src/**/*.{js,ts,tsx,jsx}'",
    "docker:prettier": "docker exec batish-app prettier --write 'src/**/*.{js,ts,tsx,jsx}'",
    "docker:run": "docker run -p 3000:3000 --name batish-app batish-money-management",
    "docker:start": "yarn run docker:build && yarn run docker:run && yarn run docker:prettier"
  },
